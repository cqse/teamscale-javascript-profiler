{
  "private": true,
  "license": "Apache-2.0",
  "name": "teamscale-javascript-profiler",
  "files": [],
  "scripts": {
    "clean": "pnpm -r --filter='./packages/*' run clean",
    "build": "pnpm -r --filter='./packages/*' run build",
    "test": "pnpm -r --filter='./packages/*' run test",
    "systemtest": "pnpm build && node ./test/systemtest.mjs",
    "systemtest:release": "pnpm build && node ./test/systemtest.mjs release",
    "systemtest:docker": "docker build -t javascript-profiler . -f systemtest.dockerfile && docker compose -f systemtest-docker-compose.yml run javascript-profiler ",
    "release": "node ./release.js",
    "lint": "pnpm exec eslint . --ext .ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "commander": "^11.0.0",
    "cypress": "^13.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-workspaces": "^0.9.0",
    "http-server": "^14.1.1",
    "local-web-server": "^5.3.0",
    "mkdirp": "^3.0.1",
    "mock-http-server": "^1.4.5",
    "prettier": "^3.0.3",
    "tempfile": "^5.0.0",
    "tree-kill": "^1.2.2",
    "typescript": "^5.2.2"
  },
  "pnpm": {
    "overrides": {
      "@types/istanbul-lib-instrument>source-map": "0.7.4",
      "@babel/traverse>@babel/types": "7.22.5",
      "@babel/parser>@babel/types": "7.22.5",
      "@types/babel__traverse>@babel/types": "7.22.5"
    },
    "patchedDependencies": {
      "mockttp@3.7.6": "patches/mockttp@3.7.6.patch"
    }
  }
}
