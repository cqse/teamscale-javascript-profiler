{
  "name": "@teamscale/coverage-collector",
  "version": "0.1.0-beta.8",
  "description": "Collector for JavaScript code coverage information",
  "main": "dist/src/main.js",
  "bin": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "author": "CQSE GmbH",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cqse/teamscale-javascript-profiler.git"
  },
  "scripts": {
    "prepublishOnly": "pnpm clean && pnpm build",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "tsc",
    "collector": "node dist/src/main.js",
    "test": "pnpm build && NODE_OPTIONS='--experimental-vm-modules' jest --coverage --silent=true --detectOpenHandles --forceExit"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@cqse/commons": "0.1.0-beta.8",
    "argparse": "^2.0.1",
    "async": "^3.2.5",
    "axios": "^1.7.2",
    "bunyan": "^1.8.15",
    "date-and-time": "^3.3.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "mkdirp": "^3.0.1",
    "rxjs": "^7.8.1",
    "source-map": "^0.7.4",
    "tmp": "^0.2.3",
    "typescript-optional": "^2.0.1",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@types/argparse": "^2.0.16",
    "@types/async": "^3.2.24",
    "@types/bunyan": "^1.8.11",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.5",
    "@types/tmp": "^0.2.6",
    "@types/ws": "^8.5.10",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.21.5",
    "jest": "^29.7.0",
    "mockttp": "^3.12.0",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}