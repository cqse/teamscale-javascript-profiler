    "date-and-time": "^2.4.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "mkdirp": "^2.1.3",
    "rxjs": "^7.8.0",
    "source-map": "^0.7.4",
    "tmp": "^0.2.1",
    "typescript-optional": "^2.0.1",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@types/argparse": "^2.0.10",
