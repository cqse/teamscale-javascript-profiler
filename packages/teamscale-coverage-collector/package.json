    "@types/ws": "^8.5.4",
    "babel-jest": "^29.4.3",
    "esbuild": "^0.17.10",
    "jest": "^29.4.3",
    "mockttp": "^3.6.3",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
