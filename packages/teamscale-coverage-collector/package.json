  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@cqse/commons": "workspace:*",
    "argparse": "^2.0.1",
    "async": "^3.2.0",
    "rxjs": "^7.1.0",
    "source-map": "^0.7.3",
    "typescript-optional": "^2.0.1",
    "winston": "^3.3.3",
    "ws": "^7.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@types/argparse": "^2.0.5",
    "@types/async": "^3.2.6",
    "@types/jest": "^27.0.1",
    "@types/node": "^15.0.1",
    "@types/source-map": "^0.5.7",
    "@types/winston": "^2.4.4",
    "@types/ws": "^7.4.2",
    "babel-jest": "^27.2.0",
    "esbuild": "^0.13.4",
    "jest": "^27.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
