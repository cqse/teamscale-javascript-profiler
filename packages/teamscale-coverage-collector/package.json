{
  "name": "@teamscale/coverage-collector",
  "version": "0.0.1-beta.42",
  "description": "Collector for JavaScript code coverage information",
  "main": "dist/src/main.js",
  "bin": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "author": "CQSE GmbH",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cqse/teamscale-javascript-profiler.git"
  },
  "scripts": {
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "tsc",
    "collector": "node dist/src/main.js",
    "test": "yarn build && NODE_OPTIONS='--experimental-vm-modules' jest --forceExit --coverage --silent=true --detectOpenHandles"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@cqse/commons": "^0.0.1-beta.26",
    "argparse": "^2.0.1",
    "async": "^3.2.4",
    "axios": "^0.24.0",
    "bunyan": "^1.8.15",
    "date-and-time": "^2.3.1",
    "dotenv": "^14.1.0",
    "form-data": "^4.0.0",
    "mkdirp": "^1.0.4",
    "rxjs": "^7.1.0",
    "source-map": "^0.7.3",
    "tmp": "^0.2.1",
    "typescript-optional": "^2.0.1",
    "ws": "^7.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@types/argparse": "^2.0.5",
    "@types/async": "^3.2.6",
    "@types/bunyan": "^1.8.8",
    "@types/jest": "^27.0.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^15.0.1",
    "@types/source-map": "^0.5.7",
    "@types/tmp": "^0.2.3",
    "@types/ws": "^7.4.2",
    "babel-jest": "^27.2.0",
    "esbuild": "^0.13.4",
    "jest": "^27.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
