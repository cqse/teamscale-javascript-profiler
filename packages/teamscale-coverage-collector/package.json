{
  "name": "@teamscale/coverage-collector",
  "version": "0.0.1-beta.57",
  "description": "Collector for JavaScript code coverage information",
  "main": "dist/src/main.js",
  "bin": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "author": "CQSE GmbH",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cqse/teamscale-javascript-profiler.git"
  },
  "scripts": {
    "prepublishOnly": "pnpm clean && pnpm build",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "tsc",
    "collector": "node dist/src/main.js",
    "test": "pnpm build && NODE_OPTIONS='--experimental-vm-modules' jest --coverage --silent=true --detectOpenHandles --forceExit"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@cqse/commons": "^0.0.1-beta.54",
    "argparse": "^2.0.1",
    "async": "^3.2.4",
    "axios": "^1.4.0",
    "bunyan": "^1.8.15",
    "date-and-time": "^3.0.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "mkdirp": "^3.0.1",
    "rxjs": "^7.8.1",
    "source-map": "^0.7.4",
    "tmp": "^0.2.1",
    "typescript-optional": "^2.0.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@types/argparse": "^2.0.10",
    "@types/async": "^3.2.20",
    "@types/bunyan": "^1.8.8",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.1",
    "@types/tmp": "^0.2.3",
    "@types/ws": "^8.5.5",
    "babel-jest": "^29.6.1",
    "esbuild": "^0.18.11",
    "jest": "^29.6.1",
    "mockttp": "^3.7.6",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}