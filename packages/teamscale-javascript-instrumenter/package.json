{
  "name": "@teamscale/javascript-instrumenter",
  "version": "0.1.0-beta.8",
  "description": "JavaScript coverage instrumenter with coverage forwarding to a collector process",
  "main": "dist/src/main.js",
  "bin": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "author": "CQSE GmbH",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cqse/teamscale-javascript-profiler.git"
  },
  "scripts": {
    "prepublishOnly": "pnpm clean && pnpm build",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "tsc --project tsconfig.json && pnpm buildVaccine",
    "buildVaccine": "node esbuild.mjs",
    "instrumenter": "node dist/src/main.js",
    "test": "pnpm build && NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=8192' jest --forceExit --coverage --silent=true --detectOpenHandles"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@types/argparse": "^2.0.16",
    "@types/async": "^3.2.24",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/bunyan": "^1.8.11",
    "@types/convert-source-map": "^2.0.3",
    "@types/glob": "^8.1.0",
    "@types/istanbul-lib-instrument": "^1.7.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.5",
    "@types/ws": "^8.5.10",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.21.5",
    "esbuild-plugin-inline-worker": "^0.1.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@cqse/commons": "0.1.0-beta.8",
    "@teamscale/lib-instrument": "workspace:../lib-instrument",
    "@babel/generator": "^7.24.7",
    "@babel/parser": "^7.24.7",
    "@babel/traverse": "^7.24.7",
    "@babel/types": "^7.24.7",
    "@types/micromatch": "^4.0.7",
    "argparse": "^2.0.1",
    "async": "^3.2.5",
    "bunyan": "^1.8.15",
    "convert-source-map": "^2.0.0",
    "foreground-child": "^3.2.1",
    "glob": "^10.4.2",
    "micromatch": "4.0.7",
    "mkdirp": "^3.0.1",
    "source-map": "0.7.4",
    "typescript-optional": "^2.0.1",
    "unload": "^2.4.1",
    "web-worker": "^1.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}