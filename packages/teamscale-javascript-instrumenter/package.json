{
  "name": "@teamscale/javascript-instrumenter",
  "version": "0.0.1-beta.51",
  "description": "JavaScript coverage instrumenter with coverage forwarding to a collector process",
  "main": "dist/src/main.js",
  "bin": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "author": "CQSE GmbH",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cqse/teamscale-javascript-profiler.git"
  },
  "scripts": {
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "tsc --project tsconfig.json && yarn buildVaccine",
    "buildVaccine": "node esbuild.mjs",
    "instrumenter": "node dist/src/main.js",
    "test": "yarn build && NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=8192' jest --forceExit --coverage --silent=true --detectOpenHandles"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@types/async": "^3.2.6",
    "@types/bunyan": "^1.8.8",
    "@types/convert-source-map": "^1.5.1",
    "@types/glob": "^7.1.3",
    "@types/istanbul-lib-instrument": "^1.7.4",
    "@types/jest": "^27.0.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^15.0.1",
    "@types/source-map": "^0.5.7",
    "@types/ws": "^7.4.4",
    "babel-jest": "^27.2.0",
    "esbuild": "^0.13.3",
    "esbuild-plugin-inline-worker": "^0.1.1",
    "jest": "^27.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslib": "^2.2.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@babel/generator": "^7.18.2",
    "@babel/parser": "^7.18.5",
    "@babel/traverse": "^7.18.6",
    "@babel/types": "^7.18.4",
    "@cqse/commons": "^0.0.1-beta.45",
    "@types/micromatch": "^4.0.2",
    "argparse": "^2.0.1",
    "async": "^3.2.4",
    "bunyan": "^1.8.15",
    "convert-source-map": "^1.7.0",
    "foreground-child": "^2.0.0",
    "glob": "^7.1.7",
    "istanbul-lib-instrument": "^5.2.0",
    "micromatch": "4.0.4",
    "mkdirp": "^1.0.4",
    "source-map": "0.7.4",
    "typescript-optional": "^2.0.1",
    "unload": "^2.2.0",
    "web-worker": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
