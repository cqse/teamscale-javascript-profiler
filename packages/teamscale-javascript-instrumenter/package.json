{
  "name": "@teamscale/javascript-instrumenter",
  "version": "0.0.1-beta.51",
  "description": "JavaScript coverage instrumenter with coverage forwarding to a collector process",
  "main": "dist/src/main.js",
  "bin": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "author": "CQSE GmbH",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cqse/teamscale-javascript-profiler.git"
  },
  "scripts": {
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "tsc --project tsconfig.json && yarn buildVaccine",
    "buildVaccine": "node esbuild.mjs",
    "instrumenter": "node dist/src/main.js",
    "test": "yarn build && NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=8192' jest --forceExit --coverage --silent=true --detectOpenHandles"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.20.2",
    "@types/async": "^3.2.18",
    "@types/bunyan": "^1.8.8",
    "@types/convert-source-map": "^2.0.0",
    "@types/glob": "^8.1.0",
    "@types/istanbul-lib-instrument": "^1.7.4",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@types/source-map": "^0.5.7",
    "@types/ws": "^8.5.4",
    "babel-jest": "^29.4.3",
    "esbuild": "^0.17.10",
    "esbuild-plugin-inline-worker": "^0.1.1",
    "jest": "^29.4.3",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@babel/generator": "^7.21.1",
    "@babel/parser": "^7.21.2",
    "@babel/traverse": "^7.21.2",
    "@babel/types": "^7.21.2",
    "@cqse/commons": "^0.0.1-beta.45",
    "@types/micromatch": "^4.0.2",
    "argparse": "^2.0.1",
    "async": "^3.2.4",
    "bunyan": "^1.8.15",
    "convert-source-map": "^2.0.0",
    "foreground-child": "^2.0.0",
    "glob": "^8.1.0",
    "istanbul-lib-instrument": "^5.2.1",
    "micromatch": "4.0.5",
    "mkdirp": "^2.1.3",
    "source-map": "0.7.4",
    "typescript-optional": "^2.0.1",
    "unload": "^2.4.1",
    "web-worker": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
